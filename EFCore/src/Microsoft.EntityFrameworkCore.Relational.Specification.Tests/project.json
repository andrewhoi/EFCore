{
  "version": "1.0.1",
  "description": "Shared test suite for Entity Framework Core relational database providers.",
  "packOptions": {
    "tags": [
      "Entity Framework Core",
      "entity-framework-core",
      "EF",
      "Data",
      "O/RM"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Specification.Tests": "1.0.1",
    "Microsoft.EntityFrameworkCore.Relational": "1.0.1"
  },
  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "dnxcore50",
        "portable-net45+win8"
      ]
    },
    "net451": {}
  }
}